{
  "name": "skuad",
  "version": "1.0.0",
  "description": "Skuad design system",
  "main": "index.js",
  "scripts": {
    "build": "lerna run --parallel --scope \"@skuad/*\" build",
    "bootstrap": "lerna bootstrap",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
